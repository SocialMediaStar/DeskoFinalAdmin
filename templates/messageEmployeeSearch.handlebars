{{#if this.data}}
<div class="w-100">
    <ul id="js-msgr-listfilter" class="list-unstyled m-0">
        {{#each data}}
        <li>
            <a href="javascript:void(0)" onClick="viewMessage(0,{{employee_id}})" class="d-table w-100 px-2 py-2 text-dark hover-white" data-filter-tags="tracey chang online">
                <div class="d-table-cell align-middle">
                    <span class="profile-image-md rounded-circle d-block" style="background-image:url('assets/img/default.png'); background-size: cover;"></span>
                </div>
                <div class="d-table-cell w-100 align-middle pl-2 pr-2">
                    <div class="text-truncate text-truncate-md">
                        {{this.user_name}}
                        <small class="d-block font-italic text-success fs-xs">
                            Viimati online: <br/>{{this.lastlogin}}
                        </small>
                    </div>
                </div>
            </a>
        </li>
        {{/each}}
    </ul>
</div>
{{/if}}